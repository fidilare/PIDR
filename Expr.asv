function [ res ] = Expr( U, V, E )
  
    res = '';
    a = {};
    length(E);
    if (isempty(U) || isempty(E))
        return
    end
    for (i = 1:length(U))
         a = chercheArc(E,U{i});
         if (length(a) ~= 0)
            for (k = 1:length(a))
                
                res = strcat(res,'(',a{k}.src,',',a{k}.dst,') & ',Expr(supprElement(U,a{k}.src),V,supprElement(E,a{k})));
            end
         end
    end


return

end

